{
    "name": "front-end",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install",
        "format": "prettier --write 'src/**/*.{js,json,tsx,css,less,scss,ts,html,md}' && git update-index --again",
        "lint:eslint": "eslint --cache --max-warnings 0 'src/**/*.{ts,tsx}'",
        "lint:lint-staged": "lint:lint-staged && git update-index --again && lint-staged",
        "lint:stylelint": "stylelint --cache --fix \"**/*.{less,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
    },
    "dependencies": {
        "next": "12.1.6",
        "next-themes": "^0.2.0",
        "react": "18.1.0",
        "react-dom": "18.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.1",
        "@commitlint/config-angular": "^17.0.0",
        "@types/node": "17.0.36",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "eslint": "^8.16.0",
        "eslint-config-next": "12.1.6",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "husky": "^8.0.0",
        "lint-staged": "^12.4.3",
        "prettier": "^2.6.2",
        "stylelint": "^14.8.5",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-config-standard-scss": "^4.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.2.0",
        "typescript": "4.7.2"
    },
    "lint-staged": {
        "*.{tsx,ts,js}": "eslint --fix"
    }
}
